                                            if (ans.IsAnswer == "yes")
                                            {
                                                check = true;
                                                <div class="row">
                                                    <span class="col-xs-12 section-title"><b>&nbsp;&nbsp; A)&nbsp;&nbsp; &nbsp;&nbsp;@(ans == null ? "N/A" : ans.Answer)</b></span>
                                                </div>
                                            }
                                        }
                                        if (check == false)
                                        {
                                            <div class="row">
                                                <span class="col-xs-12 section-title"><b>&nbsp;&nbsp; A)&nbsp;&nbsp; &nbsp;&nbsp;N/A</b></span>
                                            </div>
                                        }
                                        if (subQ.haveDateTime == "yes")
                                        {
                                            <div class="row">
                                                <span class="col-xs-12 section-title"><b>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp; Date: &nbsp;&nbsp; &nbsp;&nbsp;@(subQ.Date == null || subQ.Date == "" ? "N/A" : subQ.Date)</b></span>
                                            </div>
                                        }
                                    }
                                }
                            }





                        }



                    </div>

                </div>
            }
        </div>


    </div>
</body>
