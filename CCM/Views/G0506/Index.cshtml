    }
    /*Checkboxes styles*/
    .dynamic-form-control-checkbox {
        display: none;
    }

        .dynamic-form-control-checkbox + label {
            display: block;
            position: relative;
            padding-left: 30px;
            margin-bottom: 10px;
            cursor: pointer;
            -webkit-user-select: none;
            -moz-user-select: none;
            -ms-user-select: none;
        }

            .dynamic-form-control-checkbox + label:last-child {
                margin-bottom: 0;
            }

            .dynamic-form-control-checkbox + label:before {
                content: '';
                display: block;
                width: 15px;
                height: 15px;
                border: 1px solid #0364bd;
                position: absolute;
                left: 0;
                top: 0;
                opacity: .6;
                -webkit-transition: all .12s, border-color .08s;
                transition: all .12s, border-color .08s;
            }

        .dynamic-form-control-checkbox:checked + label:before {
            width: 6px;
            top: -1px;
            left: 8px;
            border-radius: 0;
            opacity: 1;
            border-top-color: transparent;
            border-left-color: transparent;
            -webkit-transform: rotate(45deg);
            transform: rotate(45deg);
        }

    body section:not(:nth-child(2)) {
        background-color: #e9ecef;
    }

    section ul li {
        padding: 10px;
    }

        section ul li:nth-child(even) {
            background-color: #f3f3f3;
        }

        section ul li:nth-child(odd) {
            background-color: white;
        }

        section ul li label:first-child {
            font-weight: 500;
        }
    /*.d-none{
        display:none;
    }*/

    .mainQuestion-label {
        font-weight: 900 !important;
        display: block !important;
    }

    .subQuestion-margin {
        margin-left: 25px;
    }

    .subQuestion-label {
        display: block;
        font-weight: 500;
    }

    .dynamic-inputs {
        width: max-content;
        display: inline-block;
        vertical-align: super;
    }

    .dynamic-inputs-checkbox {
        width: max-content;
        display: -webkit-inline-box;
        vertical-align: super;
    }

    .dynamic-inputs-checkbox {
        padding: 0px 0px 2px 16px;
        display: -webkit-box;
    }

        .dynamic-inputs-checkbox > label {
            margin-left: 10px;
            font-weight: 500
